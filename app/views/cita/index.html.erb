<p id="notice"><%= notice %></p>

<h1>Cita</h1>

<table>
  <thead>
    <tr>
      <th>Cod cita</th>
      <th>Cod solicitud</th>
      <th>Fecha</th>
      <th>Estado</th>
      <th>Usu crea</th>
      <th>Fec crea</th>
      <th>Usu mod</th>
      <th>Fec mod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cita.each do |citum| %>
      <tr>
        <td><%= citum.cod_cita %></td>
        <td><%= citum.cod_solicitud %></td>
        <td><%= citum.fecha %></td>
        <td><%= citum.estado %></td>
        <td><%= citum.usu_crea %></td>
        <td><%= citum.fec_crea %></td>
        <td><%= citum.usu_mod %></td>
        <td><%= citum.fec_mod %></td>
        <td><%= link_to 'Show', citum %></td>
        <td><%= link_to 'Edit', edit_citum_path(citum) %></td>
        <td><%= link_to 'Destroy', citum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Citum', new_citum_path %>
