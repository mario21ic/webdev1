
<div class="row">

   <div class="col-4 offset-4">
   <br>
   <h3>Log In</h3>
   <br>
   <%= form_for(:session, url: login_path) do |f|%>

          <%= f.label :correo_corporativo %>
          <%= f.email_field :correo_corporativo, :class => "form-control"%>

          <%= f.label :contraseña %>
          <%= f.password_field :contraseña, :class => "form-control"%>

          <br>

          <%=f.submit "Log In", :class => "btn btn-success" %>

<%end%>
         <br>
         <p>
            Tienes una cuenta? <%= link_to "Registrate Ahora", signup_path %>
         </p>

</div>

</div>